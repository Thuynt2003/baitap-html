    "version": "0.2.0",
    "configurations": [
        
    
    
    {
        "type": "lldb",
        "request": "launch",
        "name": "Cargo launch",
        "cargo": {
            "args": [
                "build",
                "--lib"
            ]
        },
        "program": "${cargo:program}",
        "args": []
    },
        {
            "name": "Attach to Chrome",
            "port": 9222,
            "request": "attach",
            "type": "chrome",
            "webRoot": "${workspaceFolder}"
        },
        
    {
        "name": "Launch Chrome",
        "request": "launch",
        "type": "chrome",
        "url": "http://localhost:8080",
        "webRoot": "${workspaceFolder}"
    },
        {
            "type": "pwa-chrome",
            "request": "launch",
            "name": "Open bai2.html",
            "file": "d:\\Bai tap\\baitap-html\\bai2.html"
        }
    ]
}